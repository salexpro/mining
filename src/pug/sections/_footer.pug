-
    const partners = ['miningclub', 'blockchain', 'emcd', 'bitcluster']

footer.footer
    .container
        .owl-carousel.footer_carousel
            each partner in partners
                .footer_carousel_item
                    img(srcset=`./assets/img/partners/${partner}@2x.png 2x, ./assets/img/partners/${partner}.png 1x` src=`./assets/img/partners/${partner}.png` alt=partner)
        .owl-nav